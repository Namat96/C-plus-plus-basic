#include <iostream>
using namespace std;
typedef std:: string text_t;
typedef int number_n;
using cgpa = double;
int main() {
    text_t name = "Namat";
    number_n age = 21;
    cgpa first_sem = 3.9;
    std:: cout << name<< endl;
    std:: cout << age<< endl;
    std:: cout << first_sem<< endl;


	return 0;
}
