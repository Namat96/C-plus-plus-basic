#include <iostream>

int main()
{
    int week = 52;
    int years = 1;
    double days = 365;
    double gpa = 3.99;
    char grade = 'A';

    std::cout << "Week: " << week << '\n';
    std::cout << "Years: " << years << '\n';
    std::cout << "Days: " << days << '\n';
    std::cout << "GPA: " << gpa << '\n';
    std::cout << "Grade: " << grade << '\n';

    return 0;
}
